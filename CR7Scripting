-- Animar la barra de progreso
local function animateProgressBar()
    for i = 0, 1, 0.01 do
        ProgressBar.Size = UDim2.new(i, 0, 1, 0)
        wait(0.01)  -- Ajuste de tiempo para una animación más fluida
    end
end

-- Mostrar la interfaz, animar la barra de progreso y luego eliminarla
animateProgressBar()
wait(1)
ScreenGui:Destroy()

